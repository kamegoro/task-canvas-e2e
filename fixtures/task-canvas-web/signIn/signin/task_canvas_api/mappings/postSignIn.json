{
  "request": {
    "method": "POST",
    "url": "/v1/signIn",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "equalToJson": {
          "mail": "test@example.com",
           "password": "password"
        }
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Authorization": "token"
    }
  }
}