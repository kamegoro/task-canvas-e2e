{
  "request": {
    "method": "POST",
    "url": "/v1/signUp",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "equalToJson": {
          "email": "test@example.com",
          "password": "password"
        }
      }
    ]
  },
  "response": {
    "fixedDelayMilliseconds": 500,
    "status": 200
  }
}
